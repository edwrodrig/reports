{
    "name": "edwrodrig/reports",
    "description" : "A library yto help to do tabular reports in HTML and CSV",
    "license" : "MIT",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.2",
	    "phpdocumentor/reflection-docblock": "4.3.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\reports\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\reports\\" : "tests/"
        }
    },
    "archive" : {
        "exclude": [
            "/examples",
            "/tests",
            "/.idea",
            ".gitignore",
            ".git",
            "phpunit.xml.dist",
            "README.md"
        ]
    }
}
